	<lrml:Statements>
		<!-- ====================================================== -->
		<!-- ==============  EMPLOYMENT CONTRACTS ================= -->
		<!-- ====================================================== -->
		<lrml:PrescriptiveStatement key="ll_art179para5pt1">
			<ruleml:Rule key=":ll_art179para5pt1" closure="universal" strength="defeasible">
				<ruleml:if>
					<ruleml:And>
						<ruleml:Atom>
							<ruleml:Rel>noLongerNeeded</ruleml:Rel>
							<ruleml:Var>Employee</ruleml:Var>
						</ruleml:Atom>
					</ruleml:And>
				</ruleml:if>
				<ruleml:then>
					<ruleml:Atom>
						<ruleml:Rel>employmentTerminated</ruleml:Rel>
						<ruleml:Var>Employee</ruleml:Var>
					</ruleml:Atom>
				</ruleml:then>
			</ruleml:Rule>
		</lrml:PrescriptiveStatement>
		<lrml:PrescriptiveStatement key="ll_art187para1">
			<ruleml:Rule key=":ll_art187para1" closure="universal" strength="defeasible">
				<ruleml:if>
					<ruleml:And>
						<ruleml:Atom>
							<ruleml:Rel>pregnant</ruleml:Rel>
							<ruleml:Var>Employee</ruleml:Var>
						</ruleml:Atom>
					</ruleml:And>
				</ruleml:if>
				<ruleml:then>
					<ruleml:Neg>
						<ruleml:Atom>
							<ruleml:Rel>employmentTerminated</ruleml:Rel>
							<ruleml:Var>Employee</ruleml:Var>
						</ruleml:Atom>
					</ruleml:Neg>
				</ruleml:then>
			</ruleml:Rule>
		</lrml:PrescriptiveStatement>
		<lrml:OverrideStatement>
			<lrml:Override under="#ll_art179para5pt1" over="#ll_art187para1"/>
		</lrml:OverrideStatement>

	</lrml:Statements>
